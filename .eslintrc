{
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "react/prop-types": 0
    }
}